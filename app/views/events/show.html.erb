
<% content_for :title, "#{@event.title}の勉強会一覧 | Tech-TV" %>
<%
  content_for :meta_description,
  "「#{@event.title}」の勉強会詳細です。Tech-TVは、過去に行われたIT勉強会のYouTubeLiveを紹介しているサイトです。参加できなかった勉強会や、気になる勉強会探しにご利用ください。"
%>
<% content_for :meta_keywords, "#{@event.title}" %>
<% content_for :og_title, "#{@event.title}の勉強会 | Tech-TV" %>
<%
  content_for :og_description,
  "「#{@event.title}」の勉強会詳細です。Tech-TVは、過去に行われたIT勉強会のYouTubeLiveを紹介しているサイトです。参加できなかった勉強会や、気になる勉強会探しにご利用ください。"
%>
<%= react_component('EventDetail', {event: @event, tags: @tags, movies: @movies, thumbnail: @thumbnail}) %>
<% if @tags.present? %>
  <%= react_component('EventDetailTagRelatedEvents', {event: @event }) %>
  <%= react_component('NewlyEvents', { hasInfiniteScroll: false }) %>
<% else %>
  <%= react_component('PopularTagRelatedEvents') %>
<% end %>